{
  "name": "@nordlys-labs/nordlys-ai-provider",
  "author": "Nordlys",
  "version": "1.2.1",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nordlys-Labs/nordlys-ai-provider.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@ai-sdk/provider": "3.0.0",
    "@ai-sdk/provider-utils": "^4.0.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@edge-runtime/vm": "^5.0.0",
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "husky": "^9.1.7",
    "prettier": "3.7.4",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16"
  },
  "bugs": {
    "url": "https://github.com/Nordlys-Labs/nordlys-ai-provider/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/**/*"
  ],
  "homepage": "https://ai-sdk.dev/docs",
  "keywords": [
    "ai",
    "nordlys",
    "provider",
    "vercel-ai",
    "vercel",
    "ai-sdk",
    "mixture of models"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "bun run clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "bun run clean && tsup --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "biome check .",
    "check:fix": "biome check . --write",
    "check:unsafe": "biome check . --write --unsafe",
    "format": "biome format . --write",
    "typecheck": "tsc --build",
    "test": "bun run test:node && bun run test:edge",
    "test:update": "bun run test:node -u",
    "test:watch": "vitest --config vitest.node.config.js",
    "test:edge": "vitest --config vitest.edge.config.js --run",
    "test:node": "vitest --config vitest.node.config.js --run",
    "test:coverage": "vitest --config vitest.node.config.js --coverage",
    "prepublishOnly": "bun run build && bun run test && bun run lint && bun run typecheck",
    "dev": "bun run build:watch",
    "ci": "bun run lint && bun run typecheck && bun run test",
    "changeset": "changeset",
    "version": "changeset version && bun run build",
    "release": "changeset publish"
  }
}
